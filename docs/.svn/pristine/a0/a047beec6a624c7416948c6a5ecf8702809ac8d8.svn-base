
TARGET=nutshell-scheme-dot

test:
	echo ${PWD} 

default: Makefile $(TARGET).pdf $(TARGET).png $(TARGET).html $(TARGET).txt
	@echo 'Ready.'

# Send to SVN repository AND update the dir in elmo.
